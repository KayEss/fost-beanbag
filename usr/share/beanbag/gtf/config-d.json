{
    "webserver": {
        "load": ["libbeanbag-views-d.so"],

        "hosts": {
            "": "raw"
        },

        "views": {
            "raw": {
                "view": "beanbag.raw",
                "configuration": {
                    "database": {
                        "name": "gtf",
                        "filepath": "gtf.json",
                        "template": "usr/share/beanbag/gtf/database.json"
                    },
                    "html": {
                        "static": "usr/share/beanbag/gtf/page.html"
                    }
                }
            }
        }
    }
}
